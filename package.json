{
  "name": "projectConfigApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "podm1": "cd ios && arch -x86_64 pod install",
    "android-dev": "react-native run-android  --variant=devDebug  --appIdSuffix dev",
    "android-dev-build": "cd android && ./gradlew clean && ENVFILE=.env.development ./gradlew assembleRelease",
    "android-release": "react-native run-android --variant=prodDebug --appIdSuffix prod",
    "android-release-build": "cd android && ./gradlew clean && ENVFILE=.env.product ./gradlew assembleRelease"
  },
  "dependencies": {
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@reduxjs/toolkit": "^1.9.5",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "axios": "^1.4.0",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-config": "^1.5.0",
    "react-native-push-notification": "^8.1.1",
    "react-navtive-segment-control": "^0.3.3",
    "react-redux": "^8.0.5",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-push-notification": "^8.1.1",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
